{
  "source": {
    "include": ["src"],
    "includePattern": "\\.(jsx|js|ts|tsx)$",
    "excludePattern": "(^|\\/|\\\\)_"
  },
  "plugins": ["node_modules/better-docs/typescript"],
  "typescript": {
    "moduleRoot": "test"
  },
  "templates": {
    "better-docs": {
      "name": "My React components"
    }
  },
  "opts": {
    "template": "./node_modules/clean-jsdoc-theme",
    "destination": "docs",
    "recurse": true,
    "readme": "README.md"
  }
}
